<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OpenLoganalyzer"
        xmlns:System="clr-namespace:System;assembly=mscorlib" x:Name="Window_OpenLoganalyzer" x:Class="OpenLoganalyzer.MainWindow"
        xmlns:p="clr-namespace:OpenLoganalyzer.Properties"
        mc:Ignorable="d"
        Title="{x:Static p:Resources.Title}" Height="450" Width="800">
    <Grid>
        <DockPanel x:Name="MainMenu" Height="19" LastChildFill="False" VerticalAlignment="Top" Margin="10,10,10,0" Grid.ColumnSpan="2">
            <Menu Height="19" VerticalAlignment="Top" Width="771">
                <MenuItem x:Name="MI_File" Header="{x:Static p:Resources.File}">
                    <MenuItem x:Name="MI_Open" Header="{x:Static p:Resources.Open}"/>
                </MenuItem>
            </Menu>
        </DockPanel>
    </Grid>
</Window>

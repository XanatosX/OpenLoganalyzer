<UserControl x:Class="OpenLoganalyzer.Windows.Controls.SimpleRename"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:OpenLoganalyzer.Windows.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>

        <StackPanel x:Name="SP_MainPanel"  Margin="0,0,0,0">
            <Label x:Name="L_Label" Content="Label" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,0,0,0"/>
            <TextBox x:Name="TB_NewName" HorizontalAlignment="Left" Height="23" Margin="0,0,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" RenderTransformOrigin="-0.631,-0.206" KeyDown="TB_NewName_KeyDown"/>
        </StackPanel>
    </Grid>
</UserControl>
